{
  "extends": "@istanbuljs/nyc-config-typescript",
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules/"],
  "extension": [".ts"],
  "reporter": ["text", "text-summary", "json-summary", "html"],
  "report-dir": "./coverage",
  "check-coverage": true,
  "branches": 95,
  "lines": 95,
  "functions": 95,
  "statements": 95
}
